generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Quiz {
  id        Int        @id @default(autoincrement())
  title     String
  questions Question[]
}

model Question {
  id      Int     @id @default(autoincrement())
  quiz    Quiz    @relation(fields: [quizId], references: [id])
  quizId  Int
  type    String
  text    String
  options String?
}
